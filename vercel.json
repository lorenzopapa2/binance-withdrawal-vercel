{
  "version": 2,
  "regions": ["hkg1"],
  "functions": {
    "api/main.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/main.py"
    }
  ]
}